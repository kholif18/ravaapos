<tbody id="categoryTbody">
    <% if (categories.length === 0) { %>
        <tr>
        <td colspan="5" class="text-center text-muted py-4">Tidak ada data kategori ditemukan.</td>
        </tr>
    <% } else {
        categories.forEach((category, index) => { %>
        <tr>
            <td><%= index + 1 %></td>
            <td><%= category.name %></td>
            <td><%= category.prefix %></td>
            <td>
            <span class="badge bg-label-info me-1"><%= category.get('productCount') || 0 %></span> Produk
            </td>
            <td>
            <button class="btn btn-sm btn-warning"
                    data-bs-toggle="modal"
                    data-bs-target="#modalEdit"
                    data-id="<%= category.id %>"
                    data-name="<%= category.name %>"
                    data-prefix="<%= category.prefix %>">
                <i class="bx bx-edit"></i>
            </button>
            <button class="btn btn-sm btn-danger btn-delete" data-id="<%= category.id %>">
                <i class="bx bx-trash"></i>
            </button>
            </td>
        </tr>
    <% }) } %>
</tbody>
